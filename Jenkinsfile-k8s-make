pipeline {
    agent {
    kubernetes {
      label 'make'
      yaml """
apiVersion: v1
kind: Pod
spec:
  containers:
  - name: gcc
    image: gcc:13.2.0
    command:
    - cat
    tty: true
"""
    }
  }
    stages {
        stage("first") {
            steps {
                container("gcc") {
                    sh """
                        make --version
                        gcc --version
                    """
                }
            }
        }
    }
}
